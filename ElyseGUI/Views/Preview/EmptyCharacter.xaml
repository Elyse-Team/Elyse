<UserControl x:Class="ElyseGUI.Views.Preview.EmptyCharacter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Preview="clr-namespace:ElyseGUI.Views.Preview"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Canvas >
            <Canvas.InputBindings>
                <MouseBinding Gesture="LeftClick" Command="{Binding DataContext.OpenProfileCommand,
             RelativeSource={RelativeSource AncestorType=WrapPanel}}" CommandParameter="{Binding}" />
            </Canvas.InputBindings>
            <Rectangle
    Width="100"
    Height="50"
    Fill="Blue"
    Stroke="Black" StrokeThickness="4"
    RadiusX="20" RadiusY="20"
    />

        </Canvas>
    </Grid>
</UserControl>
